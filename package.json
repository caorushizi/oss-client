    "normalize.css": "^8.0.1",
    "qiniu": "^7.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-file-drop": "^0.2.8",
    "react-lazyload": "^2.6.7",
    "react-transition-group": "^4.4.1",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.15",
    "uuid": "^3.4.0"
  }
